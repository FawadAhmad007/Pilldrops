{
  "name": "pilldrop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod-reinstall": "cd ios && pod deintegrate && pod cache clean --all && rm -rf Podfile.lock && pod install",
    "android-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android-aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd .. ",
    "clean": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd android && ./gradlew clean && cd .. && yarn cache clean --f && npm cache clean --f && watchman watch-del-all && watchman shutdown-server",
    "media": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/drawer": "^6.6.4",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@reduxjs/toolkit": "^1.9.7",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "axios": "1.6.1",
    "expo": "^49.0.0",
    "expo-barcode-scanner": "~12.5.3",
    "expo-camera": "~13.4.4",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-base64": "^0.2.1",
    "react-native-gesture-handler": "^2.13.3",
    "react-native-maps": "^1.8.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.3",
    "react-native-screens": "^3.26.0",
    "react-native-signature-canvas": "^4.7.1",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-vector-icons": "^10.0.0",
    "react-native-webview": "^13.6.2",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
